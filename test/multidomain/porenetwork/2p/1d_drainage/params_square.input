[TimeLoop]
DtInitial = 0.1 # [s]
CheckPoints =
TEnd = 500 # [s]

[Grid]
PoreGeometry = Cube
ThroatCrossSectionShape = Square
LowerLeft = 0
UpperRight = 4.5e-3
NumPores = 10
MeanPoreInscribedRadius = 200e-6
ThroatInscribedRadius = 100e-6
StandardDeviationPoreInscribedRadius = 0
BoundaryPoreLabels = xMin:2 xMax:1

[Problem]
VtpOutputFrequency = -1
EnableGravity = false
CapillaryPressure = 5000
NonWettingMassFlux = 5e-10

[Pnm.Problem]
Name = test_pnm_2p
RegularizationDelta = 1e-4

[Newton]
MaxSteps = 10
TargetSteps = 4
#UseLineSearch = true
MaxRelativeShift = 1e-5
MaxTimeStepDivisions = 20
NewtonOutputFilename = NewtonLog.txt

[InvasionState]
Verbosity = true
AccuracyCriterion = 0.99
#BlockNonwettingPhaseAtThroatLabel = 1
InvasionThetaThreshold = 0.001
SnapoffThetaThreshold = 0.999

[SpatialParams]
RegularizationLowSw = 0.01
RegularizationHighSw = 0.9
HighSwRegularizationMethod = Spline

[Regularization]
RegPercentage = 0.01

[Component]
LiquidKinematicViscosity = 1e-7
LiquidDensity = 1000
