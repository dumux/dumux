[TimeLoop]
DtInitial = 1e-1 # [s]
TEnd = 1.8e4 # [s]
MaxTimeStepSize = 1e0 # [s]
CheckPoints = 

[Grid]
File = ./grids/pnm_converted.dgf
PoreGeometry = Octahedron
ThroatCrossSectionShape = Square

[Problem]
Name = test_pnm_2pnc_md
VtpOutputFrequency = 100 # Write every n-th time step. 0 only writes a file if an invasion / snap-off occurred. -1 writes every step
EnableGravity = false
CapillaryPressure = 5000
UseFixedPressureAndSaturationBoundary = false
Source = 1e-5
InletPressure = 1e5
OutletPressure = 1e5

[Pnm.Problem]
Name = test_pnm_2pnc_md
RegularizationDelta = 1e-1

[Constraint.Problem]
Name = test_pnm_2p_md_constraint

[Vtk]
AddVelocity = 1

[Newton]
MaxSteps = 20
TargetSteps = 10
MaxRelativeShift = 1e-5
MaxTimeStepDivisions = 20

[InvasionState]
Verbosity = true
#AccuracyCriterion = 0.99

[SpatialParams]
HighSwRegularizationMethod = Spline
