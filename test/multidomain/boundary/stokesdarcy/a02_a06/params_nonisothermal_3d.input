[TimeLoop]
DtInitial =  1 # [s]
MaxTimeStepSize = 43200 # [s] (12 hours)
TEnd = 864000 # [s] (6 days)

[Stokes.Grid]
Positions0 = 0.100 0.360
Positions1 = 0.000 0.05  # 260 mm * 50 mm * 100 mm, assume two parallel plates
Positions2 = 0.000 0.10
Grading0 = 1.0
Grading1 = 1.0
Grading2 = 1.0
Cells0 = 26
Cells1 = 10
Cells2 = 20
#LowerLeft  = 0.100 0.00 0.00
#UpperRight = 0.360 0.05 0.10 # 260 mm * 50 mm * 100 mm, assume two parallel plates
#Cells = 26 10 20

[Darcy.Grid]
Positions0 = 0.200 0.260
Positions1 = -0.005  0.000 # 60 mm * 5 mm * 30 mm
Positions2 = 0.035 0.065
Cells0 = 6
Cells1 = 10
Cells2 = 6
Grading0 = 1.0
Grading1 = 1.0 #-1.5
Grading2 = 1.0
#LowerLeft  = 0.200 -0.005 0.035
#UpperRight = 0.260  0.000 0.065 # 60 mm * 5 mm * 30 mm
#Cells = 6 10 6

[Stokes.Problem]
Name = stokes
RefVelocity = 1.0 # [m/s]
RefPressure = 1e5 # [Pa]
refMoleFrac = 0 # [-]
RefTemperature = 313.15 # [K]

[Darcy.Problem]
Name = darcy
Pressure = 1.0e5
Saturation = 0.5 # initial Sw
InitPhasePresence = 3 # bothPhases

[SpatialParams]
Porosity = 0.41
Permeability = 2.65e-10
AlphaBJ = 1.0
Swr = 0.005
Snr = 0.01
VanGenuchtenAlpha = 6.371e-4
VanGenuchtenN = 6.9
PorousMediumTemperature = 293.15 # [K]

[Problem]
Name = test_stokes1p2cnidarcy2p2cnia02
EnableGravity = true
InterfaceDiffusionCoefficientAvg = FreeFlowOnly
EnableInertiaTerms = false

[Component]
SolidDensity = 2700
SolidThermalConductivity = 2.8
SolidHeatCapacity = 790

[Vtk]
WriteFaceData = false
OutputName = test_md_boundary_darcy2p2cni_stokes1p2cni_horizontal

[Newton]
MaxSteps = 12
MaxRelativeShift = 1e-5

[Assembly]
NumericDifference.BaseEpsilon = 1e-8
