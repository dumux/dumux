#Cells1 = 16
#Cells2 = 16
LowerLeft  = 0.100 0.00 0.00
UpperRight = 0.360 0.05 0.10 # 260 mm * 50 mm * 100 mm, assume two parallel plates
Cells = 3 20 10

[Darcy.Grid]
#Positions0 = 0.200 -0.005 0.035
#Positions1 = 0.260  0.000 0.035  # 60 mm * 5 mm * 30 mm
#Positions2 = 0.260 -0.005 0.065 
#Cells0 = 8
#Cells1 = 8
