#Grading2 = 1.0
LowerLeft  = 0.200 -0.005 0.035
UpperRight = 0.260  0.000 0.065 # 60 mm * 5 mm * 30 mm
Cells = 3 20 10

[Stokes.Problem]
Name = stokes
RefVelocity = 3.5 # [m/s]
RefPressure = 1e5 # [Pa]
refMoleFrac = 0 # [-]
RefTemperature = 298.15 # [K]

[Darcy.Problem]
Name = darcy
Pressure = 1.0e5
Saturation = 0.5 # initial Sw
InitPhasePresence = 3 # bothPhases

[SpatialParams]
Porosity = 0.41
Permeability = 2.65e-10
AlphaBJ = 1.0
Swr = 0.005
Snr = 0.01
VanGenuchtenAlpha = 6.371e-4
VanGenuchtenN = 6.9
PorousMediumTemperature = 298.15 # [K]

[Problem]
Name = test_stokes1p2cnidarcy2p2cnia02
EnableGravity = true
InterfaceDiffusionCoefficientAvg = FreeFlowOnly
EnableInertiaTerms = false

[Component]
SolidDensity = 2700
SolidThermalConductivity = 2.8
SolidHeatCapacity = 790

[Vtk]
WriteFaceData = false
OutputName = test_md_boundary_darcy2p2cni_stokes1p2cni_horizontal

[Newton]
MaxSteps = 12
MaxRelativeShift = 1e-5

[Assembly]
NumericDifference.BaseEpsilon = 1e-8
