[TimeLoop]
DtInitial =  10 # [s]
MaxTimeStepSize = 43200 # [s] (12 hours)
TEnd = 864000 # [s] (6 days)

[Stokes.Grid]
#Positions0 = 0.100 0.00 0.0
#Positions1 = 0.360 0.05 0.0   # 260 mm * 50 mm * 100 mm, assume two parallel plates
#Positions2 = 0.360 0.00 0.1
#Grading0 = 1.0
#Grading1 = 1.0
#Grading2 = 1.0
