[TimeLoop]
DtInitial =  10 # [s]
MaxTimeStepSize = 43200 # [s] (12 hours)
TEnd = 864000 # [s] (6 days)

[Stokes.Grid]
Positions0 = 0.0 0.75
Positions1 = 0.25 0.5
Grading0 = 1.0
Grading1 = 1.0 #1.5
Cells0 = 8
Cells1 = 64

[Darcy.Grid]
Positions0 = 0.25 0.50
Positions1 = 0.0 0.25
Cells0 = 8
Cells1 = 16
Grading0 = 1.0
Grading1 = 1.0 #-1.5

[Stokes.Problem]
Name = stokes
RefVelocity = 1 # [m/s]
RefPressure = 1e5 # [Pa]
refMoleFrac = 0 # [-]
RefTemperature = 298.15 # [K]

[Darcy.Problem]
Name = darcy
Pressure = 1.0e5
Saturation = 1.0 # initial Sw
InitPhasePresence = 1 # bothPhases

[SpatialParams]
Porosity = 0.41
Permeability = 2.65e-10
AlphaBJ = 1.0
Swr = 0.005
Snr = 0.01
VanGenuchtenAlpha = 6.371e-4
VanGenuchtenN = 6.9
PorousMediumTemperature = 298.15 # [K]

[Problem]
Name = test_stokes1p2cdarcy2p2chorizontal_2d
EnableGravity = true
InterfaceDiffusionCoefficientAvg = FreeFlowOnly
EnableInertiaTerms = false

[Vtk]
WriteFaceData = false
OutputName = test_md_boundary_darcy2p2c_stokes1p2c_horizontal_2d

[Newton]
MaxSteps = 12
MaxRelativeShift = 1e-5

[Assembly]
NumericDifference.BaseEpsilon = 1e-8

[SpatialParams]
Temperature = 298.15
