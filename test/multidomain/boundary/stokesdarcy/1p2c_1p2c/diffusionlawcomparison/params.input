[TimeLoop]
TEnd = 3e6 # s
DtInitial = 100 # s

[Darcy.Grid]
UpperRight = 1.0 1.0
Cells = 20 20

[Stokes.Grid]
LowerLeft = 0.0 1.0
UpperRight = 1.0 2.0
Cells = 20 20

[Stokes.Problem]
Name = stokes
Velocity = 1e-6
Pressure = 1.0e5
InletMoleFraction = 1e-3
