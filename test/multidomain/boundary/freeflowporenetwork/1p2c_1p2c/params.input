[TimeLoop]
TEnd = 10
DtInitial = 1e-3
MaxTimeStepSize = 1

[PNM.Grid]
NumPores = 2
LowerLeft = 2.5e-3 0
UpperRight = 2.5e-3 2e-3
PoreInscribedRadius = 1e-4
PoreGeometry = Cube
ThroatCrossSectionShape = Square
BoundaryPoreLabels = xMin:0 xMax:1 yMin:2 yMax:3
PriorityList = 2 3 0 1

[FreeFlow.Grid]
LowerLeft = 0 2e-3
UpperRight = 5e-3 4e-3
UpstreamCells0 = 10
CellsPerPore = 3
DownstreamCells0 = 10
Cells1 = 20

[Problem]
EnableGravity = false
OnlyDiffusion = false

[FreeFlow.Problem]
Name = ff
PressureDifference = 1
InitialPressure = 1e5
InitialMoleFraction = 0.0
EnableInertiaTerms = false

[PNM.Problem]
Name = pnm
InitialPressure = 98053.5
InitialMoleFraction = 2e-3
InitialSaturation = 8e-1

[Vtk]
OutputName = test_md_boundary_ff1p2c_pnm2p2c
AddVelocity = true

[Assembly]
NumericDifference.BaseEpsilon = 1e-4
