[PNM.Grid]
LowerLeft = 5e-3 0 0
UpperRight = 5e-3 0 2e-3
NumPores = 2
PoreInscribedRadius = 5e-4
ThroatInscribedRadius = 5e-4
BoundaryPoreLabels = xMin:1 xMax:2 yMin:3 yMax:4 zMin:5 zMax:6
PriorityList =  4 5 2 3 0 1 #zMin > zMax > yMin > yMax > xMin > xMax
PoreGeometry = Cube
ThroatCrossSectionShape = Square

[FreeFlow.Grid]
LowerLeft = -5e-3 -3e-3 2e-3
UpperRight =  1.5e-2 3e-3 5e-3
UpstreamCells0 = 2
UpstreamCells1 = 2
CellsPerPore = 1
DownstreamCells0 = 2
DownstreamCells1 = 2
Cells2 = 6

[Problem]
EnableGravity = false

[FreeFlow.Problem]
Name = ff
InletPressure = 1.01e5
OutletPressure = 1e5
EnableInertiaTerms = false

[PNM.Problem]
Name = pnm
InletPressure = 1.01e5
OutletPressure = 1e5
VerticalFlow = true

[Vtk]
OutputName = test_3d_single_throat

[Component]
LiquidDensity = 1e3
LiquidKinematicViscosity = 1e-6

[Assembly]
NumericDifference.BaseEpsilon = 1e-4

[SpatialParams]
Temperature = 298.0

[Newton]
MaxSteps = 100
MaxRelativeShift = 1e-5
