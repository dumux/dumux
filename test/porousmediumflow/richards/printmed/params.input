[TimeLoop]
DtInitial = 1e-6 # [s]
TEnd = 10 # [s]
MaxTimeStepSize = 10
InkJetFrequency = 1e-3 #Hz

[Grid]
LowerLeft = 0 0 0
UpperRight = 1e-2 1e-3 1e-2
Cells = 100 5 100

[Tablet]
Radius = 5e-3

[Drop]
InitialContactAngle = 90      # Â°
#InitialVolume = 1e-1         # m3
InitialContactRadius = 5e-4   # m
InitialCenter = 5e-3 1e-3 5e-3
DistanceBetweenDroplets = 1.5e-3

[SpatialParams]
Permeability = 8.17083e-11
TabulatedPropertiesName = "X-dir"
Swr = 0.05
VanGenuchtenAlpha = 2e-4
VanGenuchtenN = 4.7
#Permeability = 1e-15

[Problem]
Name = 2p_VanGenuchten
EnableGravity = true

[Vtk]
WriteFaceData = false
AddVelocity = true

[Newton]
TargetSteps = 18 # set the "desirable" number of Newton iterations of a time step
EnableChop = true # chop for better convergence

[LinearSolver]
Type = bicgstabsolver

[LinearSolver.Preconditioner]
Type = amg
AmgMaxLevel = 2
AmgAccumulationMode = atOnce
