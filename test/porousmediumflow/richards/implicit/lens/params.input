[TimeLoop]
DtInitial = 100 # [s]
TEnd = 30000 # [s]

[Grid]
UpperRight = 6 4
Cells = 24 16
Overlap = 3

[Problem]
Name = richardslens
EnableGravity = 1 # enable gravity

[Newton]
TargetSteps = 18 # set the "desirable" number of Newton iterations of a time step
EnableChop = true # chop for better convergence
