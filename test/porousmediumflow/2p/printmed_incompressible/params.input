[TimeLoop]
DtInitial = 1e-5 # [s]
TEnd = 10 # [s]
MaxTimeStepSize = 10
InkJetFrequency = 1000 #Hz

[Grid]
LowerLeft = 0 0
UpperRight = 1e-3 5e-4 #1e-3
Cells = 50 50 #50

[Drop]
InitialContactAngle = 90
# InitialVolume = 1e-1
InitialContactRadius = 2e-4
InitialCenter = 5e-4 5e-4 #5e-4

[SpatialParams.Outer]
Swr = 0.05
VanGenuchtenAlpha = 1e-6
VanGenuchtenN = 4.7
Permeability = 1e-18

[Problem]
Name = 2p
EnableGravity = true

[Vtk]
WriteFaceData = false
AddVelocity = true

[Newton]
EnablePartialReassembly = false
