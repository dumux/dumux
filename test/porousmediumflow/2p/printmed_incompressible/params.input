[TimeLoop]
DtInitial = 1e-5 # [s]
TEnd = 10 # [s]
MaxTimeStepSize = 10
InkJetFrequency = 1000 #Hz

[Grid]
LowerLeft = 0 0 0
UpperRight = 5e-3 1e-3 5e-3
Cells = 40 10 40

[Tablet]
Radius = 2.5e-3

[Drop]
InitialContactAngle = 90      # Â°
#InitialVolume = 1e-1         # m3
InitialContactRadius = 4e-4   # m
InitialCenter = 5e-4 1e-3 2.5e-3
DistanceBetweenDroplets = 2e-3

[SpatialParams.Outer]
Swr = 0.05
VanGenuchtenAlpha = 1e-5
VanGenuchtenN = 4.7
Permeability = 1e-15

[Problem]
Name = 2p
EnableGravity = true

[Vtk]
WriteFaceData = false
AddVelocity = true

[Newton]
EnablePartialReassembly = false
