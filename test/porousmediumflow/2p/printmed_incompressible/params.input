[TimeLoop]
DtInitial = 1e-8 # [s]
TEnd = 98 # [s]
MaxTimeStepSize = 1
InkJetFrequency = 0.02 #Hz

[Grid]
LowerLeft = 0 0 0
UpperRight = 1e-2 5e-3 1e-2
Cells = 100 25 100

[Tablet]
Radius = 5e-3

[Drop]
InitialContactAngle = 40      # Â°
InitialVolume = 5e-9        # m3
#InitialContactRadius = 5e-4   # m
InitialCenter = 5e-3 1e-3 5e-3
DistanceBetweenDroplets = 1.5e-3
DropletGravity = true

[SpatialParams]
Permeability = 2.4147e-18
TabulatedPropertiesName = "Y-dir"
Swr = 0.0317364
VanGenuchtenAlpha = 2e-4
VanGenuchtenN = 4.7
#Permeability = 1e-15

[Problem]
Name = 2p_tabulated
EnableGravity = true
VtpOutputFrequency = 10 # Write every n-th time step.


[Vtk]
WriteFaceData = false
AddVelocity = true

[Newton]
EnablePartialReassembly = false
TargetSteps = 10 # set the "desirable" number of Newton iterations of a time step
EnableChop = false # chop for better convergence
