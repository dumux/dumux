[TimeLoop]
DtInitial = 1e-3 # [s]
TEnd = 30 # [s]

[Grid]
File = micromodel_network.dgf
PoreGeometry = Cube
ThroatCrossSectionShape = Square
AddThroatVolumeToPoreVolume = true


[Problem]
Name = pnm_3d_comparison_manchester_2
VtpOutputFrequency = 100 # Write every n-th time step. 0 only writes a file if an invasion / snap-off occured. -1 writes every step
EnableGravity = false
CapillaryPressure = 7000
UseFixedPressureAndSaturationBoundary = false
DistributeByVolume = true
NonWettingMassFlux = 1.3889e-7

[Vtk]
AddVelocity = 1

[Newton]
UseLineSearch = true
MaxSteps = 20
TargetSteps = 10
MaxRelativeShift = 1e-6
MaxTimeStepDivisions = 10
LogFileName = NewtonLog.txt

[InvasionState]
Verbosity = true

[SpatialParams]
RegularizationLowSw = 0.01
RegularizationHighSw = 0.99
HighSwRegularizationMethod = Spline
SurfaceTension = 0.0536

[Component]
LiquidDynamicViscosity = 4.7e-3
LiquidDensity = 1860

[Regularization]
RegPercentage = 0.05
