###############################################################
# Parameter file for test_1p.
# Everything behind a '#' is a comment.
# Type "./test_1p --help" for more information.
###############################################################

###############################################################
# Mandatory arguments
###############################################################

[TimeManager]
DtInitial = 0 # [s]
TEnd = 0 # [s]

[Problem]
Delta = 1e-6 # [-] if this is commented in, it is used

[Grid]
NumberOfCellsX = 1 # [-] resolution in x-direction
NumberOfCellsY = 1 # [-] resolution in y-direction

UpperRightX = 1 # [m] length of the domain
UpperRightY = 1 # [m] height of the domain

NumRefine = 3 # refinement level of the created grid

###############################################################
# Simulation restart
#
# DuMux simulations can be restarted from *.drs files
# Set Restart to the value of a specific file, 
# e.g.:  'Restart = 27184.1' for the restart file
# name_time=27184.1_rank=0.drs
# Please comment in the two lines below, if restart is desired.
###############################################################
# [TimeManager]
# Restart = ... 
