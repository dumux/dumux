[TimeLoop]
DtInitial = 1e-4 # [s]
TEnd = 1e-2 # [s]

[Grid]
LowerLeft = 0 0
UpperRight = 1 1
Cells = 50 50

[Problem]
Name = test_angeli # name passed to the output routines
EnableGravity = false
PrintL2Error = false
EnableInertiaTerms = true
UseVelocityAveragingForInitial = false
UseVelocityAveragingForDirichlet = false

[Component]
LiquidDensity = 1
LiquidKinematicViscosity = 0.1

[Assembly]
NumericDifference.BaseEpsilon = 1e-8

[Newton]
MaxSteps = 10
MaxRelativeShift = 1e-10

[Vtk]
WriteFaceData = false

[Flux]
UpwindWeight = 0.5
